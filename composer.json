{
    "name": "florianwolters/component-core-stringutils",
    "description": "Offers operations on the data type string as a PHP component.",
    "type": "library",
    "keywords": ["string", "language", "helper", "wrapper"],
    "homepage": "http://github.com/FlorianWolters/PHP-Component-Core-StringUtils",
    "license": "LGPL-3.0+",
    "authors": [
      {
        "name": "Florian Wolters",
        "email": "wolters.fl@gmail.com",
        "homepage": "http://blog.florianwolters.de",
        "role": "Developer"
      }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "pdepend/pdepend": "2.*",
        "phpmd/phpmd": "2.*",
        "phpunit/phpunit": "4.*",
        "sami/sami": "2.*",
        "sebastian/phpcpd": "2.*",
        "sebastian/phpdcd": "1.*",
        "satooshi/php-coveralls": "0.*@dev",
        "sensiolabs/security-checker": "2.*",
        "squizlabs/php_codesniffer": "2.*@dev"
